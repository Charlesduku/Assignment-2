def encrypt(text, key):
    encrypted_text = ""
    for char in text:
        if char.isalpha():
            shifted = ord(char) + key
            if shifted > ord('z'):
                shifted -= 26
            elif shifted < ord('a'):
                shifted += 26
            encrypted_text += chr(shifted)
        else:
            encrypted_text += char
    return encrypted_text

key = 3  # Example key
original_code = "your original code here"
encrypted_code = encrypt(original_code, key)
print(encrypted_code)
